{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "cypress"
        ],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "outDir": "./dist"
    },
    "include": [
        "cypress/**/*.ts"
    ]
}